{
	"name": "server",
	"main": "src/index.ts",
	"type": "module",
	"scripts": {
		"build": "tsdown",
		"check-types": "tsc -b",
		"compile": "bun build --compile --minify --sourcemap --bytecode ./src/index.ts --outfile server",
		"dev": "bun run --watch src/index.ts",
		"start": "bun run dist/index.js"
	},
	"dependencies": {
		"@ai-sdk/google": "^2.0.11",
		"ai": "^5.0.30",
		"dotenv": "^17.2.1",
		"hono": "^4.8.2",
		"zod": "^4.1.5"
	},
	"devDependencies": {
		"tsdown": "^0.14.1",
		"typescript": "^5.8.2",
		"@types/bun": "^1.2.6"
	}
}
